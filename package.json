{
  "name": "react-datepicker",
  "version": "1.0.0",
  "description": "A datepicker library for the web.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tresko/react-datepicker.git"
  },
  "keywords": [
    "react",
    "datepicker",
    "styled-components",
    "react",
    "emotion",
    "hooks"
  ],
  "scripts": {
    "commit": "git-cz",
    "format": "lerna run format",
    "tsc": "lerna run tsc",
    "prebuild": "lerna run prebuild",
    "build": "lerna run build",
    "bootstrap": "lerna bootstrap"
  },
  "author": "Miha Sedej",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tresko/react-datepicker/issues"
  },
  "homepage": "https://github.com/tresko/react-datepicker#readme",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/react": "^16.8.8",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "babel-eslint": "^10.0.1",
    "cz-lerna-changelog": "^2.0.2",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "lerna": "^3.13.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "react": "^16.8.5",
    "rimraf": "^2.6.3",
    "rollup": "^1.7.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-typescript2": "^0.20.1",
    "typescript": "^3.3.4000"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-lerna-changelog"
    },
    "validate-commit-msg": {
      "helpMessage": "Commit message violates the rules defined for this project. Please, execute `yarn commit` to generate a correct commit message with `commitizen`"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn tsc && lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  }
}
