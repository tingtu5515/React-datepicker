{
  "name": "react-datepicker",
  "version": "1.0.0",
  "description": "A datepicker library for the web.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tresko/react-datepicker.git"
  },
  "keywords": [
    "react",
    "datepicker",
    "styled-components",
    "react",
    "emotion",
    "hooks"
  ],
  "scripts": {
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test:generate-output": "jest --json --outputFile=jest-test-results.json",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "commit": "git-cz",
    "format": "lerna run format",
    "lint:css": "stylelint 'packages/**/*.ts{,x}'",
    "lint:ts": "eslint 'packages/**/*.ts{,x}'",
    "lint": "run-p -c lint:*",
    "tsc": "lerna run tsc",
    "prebuild": "yarn run tsc && yarn run lint && lerna run prebuild",
    "build": "lerna run build",
    "build:watch": "lerna run build:watch",
    "bootstrap": "lerna bootstrap",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Miha Sedej",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tresko/react-datepicker/issues"
  },
  "homepage": "https://github.com/tresko/react-datepicker#readme",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@storybook/addon-actions": "^5.1.0-alpha.15",
    "@storybook/addon-links": "^5.1.0-alpha.15",
    "@storybook/addon-knobs": "^5.1.0-alpha.15",
    "@storybook/addons": "^5.1.0-alpha.15",
    "@storybook/react": "^5.1.0-alpha.15",
    "@types/jest": "^24.0.11",
    "@types/storybook__addon-knobs": "^4.0.4",
    "@types/react": "^16.8.8",
    "@types/storybook__react": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "cz-lerna-changelog": "^2.0.2",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^1.3.1",
    "is-ci-cli": "^1.1.1",
    "jest": "^24.5.0",
    "jest-dom": "^3.1.3",
    "jest-environment-jsdom": "^24.5.0",
    "jest-environment-node": "^24.5.0",
    "jest-runner-eslint": "^0.7.3",
    "jest-serializer-html": "^6.0.0",
    "jest-styled-components": "^6.3.1",
    "jest-watch-select-projects": "^0.1.1",
    "jest-watch-typeahead": "^0.2.1",
    "lerna": "^3.13.1",
    "lint-staged": "^8.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-testing-library": "^6.0.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.7.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-typescript2": "^0.20.1",
    "styled-components": "^4.2.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.2",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.4000"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-lerna-changelog"
    },
    "validate-commit-msg": {
      "helpMessage": "Commit message violates the rules defined for this project. Please, execute `yarn commit` to generate a correct commit message with `commitizen`"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn tsc && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "prettier --write",
      "eslint",
      "stylelint",
      "git add"
    ]
  },
  "dependencies": {}
}
